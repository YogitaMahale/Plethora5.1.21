            <h4 class="gl-cats-title">
                <a asp-area="Admin" asp-controller="Home" asp-action="BusinessListing" asp-route-businessid="@obj.id" asp-route-productid="0">
                    @obj.name
                </a>


            </h4>
        </div>


    }


</div>


