
@model IEnumerable<plathora.Models.Dtos.getAdvertisementSliderImagebySectorandCity>

<section class="sptb">
    <div class="container">

        <div class="row">


            <div class="col-md-12">

                <div class="carousel slide" id="carousel-554496">
                    <ol class="carousel-indicators">
                        @{
                            var index = 0;
                        }
                        @if (Model != null)
                        {

                            @foreach (var item in Model)
                            {
                                @if (index == 0)
                                {
                                    <li data-slide-to="@index" data-target="#carousel-554496" class="active"> </li>
                                }
                                else
                                {
                                    <li data-slide-to="@index" data-target="#carousel-554496"> </li>
                                }
                                index++;

                            }
